<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f0f8ff"/>
  <g transform="translate(400,300)">
    <g transform="translate(-150,-100)">
      <!-- IoT Device Icon -->
      <rect x="0" y="0" width="100" height="60" rx="10" fill="#4a90e2" stroke="#2171cd" stroke-width="2"/>
      <circle cx="20" cy="30" r="5" fill="#ffffff"/>
      <circle cx="40" cy="30" r="5" fill="#ffffff"/>
      <rect x="60" y="20" width="30" height="20" rx="5" fill="#ffffff"/>
      <text x="50" y="90" font-family="Arial" font-size="16" text-anchor="middle" fill="#333333">IoT Device</text>
    </g>
    
    <g transform="translate(50,-100)">
      <!-- AI Brain Icon -->
      <circle cx="50" cy="30" r="40" fill="#ff6b6b" stroke="#e74c3c" stroke-width="2"/>
      <path d="M30,20 Q50,0 70,20 Q90,40 70,60 Q50,80 30,60 Q10,40 30,20" fill="none" stroke="#ffffff" stroke-width="3"/>
      <text x="50" y="90" font-family="Arial" font-size="16" text-anchor="middle" fill="#333333">AI</text>
    </g>
    
    <!-- Connection Lines -->
    <line x1="-100" y1="-70" x2="50" y2="-70" stroke="#333333" stroke-width="2" stroke-dasharray="5,5"/>
    
    <!-- Ship Icon -->
    <g transform="translate(-50,50)">
      <path d="M0,40 L30,40 L40,20 L100,20 L110,40 L140,40 L120,60 L20,60 Z" fill="#34495e" stroke="#2c3e50" stroke-width="2"/>
      <rect x="50" y="0" width="20" height="20" fill="#34495e"/>
      <text x="70" y="90" font-family="Arial" font-size="16" text-anchor="middle" fill="#333333">Smart Ship</text>
    </g>
    
    <!-- Title -->
    <text x="0" y="-150" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle" fill="#333333">Maritime AIoT Technology</text>
    
    <!-- Data Flow Arrows -->
    <path d="M-50,-50 Q0,0 -50,50" fill="none" stroke="#27ae60" stroke-width="2" marker-end="url(#arrow)"/>
    <path d="M50,-50 Q0,0 50,50" fill="none" stroke="#27ae60" stroke-width="2" marker-end="url(#arrow)"/>
    
    <!-- Arrow Marker Definition -->
    <defs>
      <marker id="arrow" markerWidth="10" markerHeight="10" refX="9" refY="3" orient="auto" markerUnits="strokeWidth">
        <path d="M0,0 L0,6 L9,3 z" fill="#27ae60"/>
      </marker>
    </defs>
  </g>
</svg>