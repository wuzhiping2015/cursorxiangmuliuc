<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#f5f7fa"/>
  
  <!-- Background Grid -->
  <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
    <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#e6e9f0" stroke-width="1"/>
  </pattern>
  <rect width="800" height="600" fill="url(#grid)"/>
  
  <!-- Main Brain Icon -->
  <g transform="translate(400,250)">
    <!-- Brain Outline -->
    <path d="M-100,0 C-100,-80 -30,-100 0,-60 C30,-100 100,-80 100,0 C100,80 30,100 0,120 C-30,100 -100,80 -100,0 Z" 
          fill="#6c5ce7" stroke="#5541d8" stroke-width="3"/>
    
    <!-- Brain Details -->
    <path d="M0,-60 C-20,-30 -50,-40 -60,-10 C-70,20 -40,30 -30,50 C-20,70 0,80 0,120" 
          fill="none" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
    <path d="M0,-60 C20,-30 50,-40 60,-10 C70,20 40,30 30,50 C20,70 0,80 0,120" 
          fill="none" stroke="#ffffff" stroke-width="2" stroke-linecap="round"/>
    
    <!-- Neural Network Connections -->
    <g stroke="#ffffff" stroke-width="1.5">
      <line x1="-70" y1="-20" x2="-20" y2="-40" opacity="0.6"/>
      <line x1="-60" y1="10" x2="-10" y2="-20" opacity="0.6"/>
      <line x1="-50" y1="40" x2="0" y2="0" opacity="0.6"/>
      <line x1="-30" y1="60" x2="20" y2="30" opacity="0.6"/>
      <line x1="70" y1="-20" x2="20" y2="-40" opacity="0.6"/>
      <line x1="60" y1="10" x2="10" y2="-20" opacity="0.6"/>
      <line x1="50" y1="40" x2="0" y2="0" opacity="0.6"/>
      <line x1="30" y1="60" x2="-20" y2="30" opacity="0.6"/>
    </g>
    
    <!-- Neural Nodes -->
    <g fill="#ffffff">
      <circle cx="-70" cy="-20" r="4"/>
      <circle cx="-60" cy="10" r="4"/>
      <circle cx="-50" cy="40" r="4"/>
      <circle cx="-30" cy="60" r="4"/>
      <circle cx="-20" cy="-40" r="4"/>
      <circle cx="-10" cy="-20" r="4"/>
      <circle cx="0" cy="0" r="4"/>
      <circle cx="20" cy="30" r="4"/>
      <circle cx="70" cy="-20" r="4"/>
      <circle cx="60" cy="10" r="4"/>
      <circle cx="50" cy="40" r="4"/>
      <circle cx="30" cy="60" r="4"/>
      <circle cx="20" cy="-40" r="4"/>
      <circle cx="10" cy="-20" r="4"/>
    </g>
  </g>
  
  <!-- Digital Elements -->
  <g transform="translate(400,400)" font-family="Courier New" fill="#2d3436">
    <text x="-180" y="0" font-size="12">01001010</text>
    <text x="-100" y="20" font-size="12">11100101</text>
    <text x="-20" y="40" font-size="12">01011010</text>
    <text x="60" y="20" font-size="12">10101100</text>
    <text x="140" y="0" font-size="12">00110101</text>
  </g>
  
  <!-- Title -->
  <text x="400" y="500" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle" fill="#2d3436">Maritime AI Technology</text>
  
  <!-- Floating Icons -->
  <g transform="translate(150,150)">
    <circle cx="0" cy="0" r="30" fill="#74b9ff" opacity="0.8"/>
    <text x="0" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Predictive</text>
  </g>
  
  <g transform="translate(650,150)">
    <circle cx="0" cy="0" r="30" fill="#ff7675" opacity="0.8"/>
    <text x="0" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Analytics</text>
  </g>
  
  <g transform="translate(200,450)">
    <circle cx="0" cy="0" r="30" fill="#55efc4" opacity="0.8"/>
    <text x="0" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Learning</text>
  </g>
  
  <g transform="translate(600,450)">
    <circle cx="0" cy="0" r="30" fill="#ffeaa7" opacity="0.8"/>
    <text x="0" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Decision</text>
  </g>
</svg>