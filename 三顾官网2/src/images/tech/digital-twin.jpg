<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#ecf0f1"/>
  
  <!-- Background Grid -->
  <pattern id="grid" width="50" height="50" patternUnits="userSpaceOnUse">
    <path d="M 50 0 L 0 0 0 50" fill="none" stroke="#bdc3c7" stroke-width="0.5"/>
  </pattern>
  <rect width="800" height="600" fill="url(#grid)"/>
  
  <!-- Title -->
  <text x="400" y="50" font-family="Arial" font-size="24" font-weight="bold" text-anchor="middle" fill="#2c3e50">Digital Twin Technology for Maritime</text>
  
  <!-- Physical Ship -->
  <g transform="translate(200,250)">
    <!-- Ship Body -->
    <path d="M0,40 L50,40 L70,0 L180,0 L200,40 L250,40 L220,80 L30,80 Z" fill="#34495e" stroke="#2c3e50" stroke-width="2"/>
    <rect x="100" y="-30" width="30" height="30" fill="#34495e"/>
    
    <!-- Label -->
    <text x="125" y="120" font-family="Arial" font-size="18" text-anchor="middle" fill="#2c3e50">Physical Asset</text>
  </g>
  
  <!-- Digital Ship -->
  <g transform="translate(550,250)">
    <!-- Digital Ship Outline -->
    <path d="M0,40 L50,40 L70,0 L180,0 L200,40 L250,40 L220,80 L30,80 Z" fill="none" stroke="#3498db" stroke-width="2" stroke-dasharray="5,3"/>
    <rect x="100" y="-30" width="30" height="30" fill="none" stroke="#3498db" stroke-width="2" stroke-dasharray="5,3"/>
    
    <!-- Digital Elements -->
    <g font-family="Courier New" fill="#3498db" font-size="8">
      <text x="40" y="30">01001</text>
      <text x="90" y="10">10110</text>
      <text x="150" y="30">11001</text>
      <text x="70" y="60">01101</text>
      <text x="130" y="70">10011</text>
    </g>
    
    <!-- Label -->
    <text x="125" y="120" font-family="Arial" font-size="18" text-anchor="middle" fill="#3498db">Digital Twin</text>
  </g>
  
  <!-- Connection Between Physical and Digital -->
  <g transform="translate(400,250)">
    <!-- Data Flow Arrows -->
    <path d="M-70,0 L70,0" stroke="#27ae60" stroke-width="2" stroke-dasharray="10,5"/>
    <polygon points="60,-5 70,0 60,5" fill="#27ae60"/>
    
    <path d="M70,20 L-70,20" stroke="#e74c3c" stroke-width="2" stroke-dasharray="10,5"/>
    <polygon points="-60,15 -70,20 -60,25" fill="#e74c3c"/>
    
    <!-- Data Flow Labels -->
    <text x="0" y="-10" font-family="Arial" font-size="12" text-anchor="middle" fill="#27ae60">Sensor Data</text>
    <text x="0" y="40" font-family="Arial" font-size="12" text-anchor="middle" fill="#e74c3c">Control Commands</text>
  </g>
  
  <!-- Data Processing Elements -->
  <g transform="translate(400,400)">
    <!-- Cloud Computing Icon -->
    <path d="M-100,0 Q-80,-30 -50,-30 Q-40,-60 0,-60 Q40,-60 50,-30 Q80,-30 100,0 Q80,30 0,30 Q-80,30 -100,0 Z" 
          fill="#9b59b6" stroke="#8e44ad" stroke-width="2"/>
    <text x="0" y="5" font-family="Arial" font-size="14" text-anchor="middle" fill="#ffffff">Data Analytics</text>
    
    <!-- Connection to Digital Twin -->
    <path d="M0,-60 L0,-120 L150,-120 L150,-80" stroke="#95a5a6" stroke-width="2" stroke-dasharray="5,3"/>
    <polygon points="145,-90 150,-80 155,-90" fill="#95a5a6"/>
  </g>
  
  <!-- Benefits Boxes -->
  <g transform="translate(200,500)">
    <rect x="-150" y="-20" width="120" height="40" rx="5" fill="#3498db" opacity="0.7"/>
    <text x="-90" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Predictive Maintenance</text>
  </g>
  
  <g transform="translate(400,500)">
    <rect x="-150" y="-20" width="120" height="40" rx="5" fill="#2ecc71" opacity="0.7"/>
    <text x="-90" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Performance Optimization</text>
  </g>
  
  <g transform="translate(600,500)">
    <rect x="-150" y="-20" width="120" height="40" rx="5" fill="#e74c3c" opacity="0.7"/>
    <text x="-90" y="5" font-family="Arial" font-size="12" text-anchor="middle" fill="#ffffff">Remote Monitoring</text>
  </g>
</svg>